    <titlePage style="text-align: center" corresp="#strofa_N_verso_N">
      <docTitle>
       <titlePart type="main" corresp="#titolo_principale">
        IDEE PER TRE POESIE
      </titlePart>
       <titlePart type="sub" corresp="#sottotitolo">
        BALENATE A
        <persName ref="#nome_autore">
          <forename>FULVIO</forename>
          <surname>PANZERI</surname>
        </persName>
       </titlePart>
